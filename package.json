{
  "name": "react-ts-h5-template",
  "version": "1.0.0",
  "private": false,
  "license": "ISC",
  "author": "swcbo",
  "type": "module",
  "scripts": {
    "build:dev": "vite build --mode dev",
    "build:pre": "vite build --mode pre",
    "build:prod": "vite build --mode prod",
    "clean:cache": "rimraf node_modules/.cache/ && rimraf node_modules/.vite",
    "dev": "vite --mode dev",
    "lint": "npm run lint:style && npm run lint:prettier",
    "lint-staged": "lint-staged",
    "lint-staged:js": "eslint --ext .js,.jsx,.ts,.tsx ",
    "lint:fix": "eslint --cache --ext .js,.jsx,.ts,.tsx --no-error-on-unmatched-pattern --quiet --fix ./src",
    "lint:prettier": "prettier --check 'src/*' --end-of-line auto",
    "lint:style": "stylelint --fix \"src/**/*.less\" --syntax less",
    "pre": "vite --mode pre",
    "precommit": "lint-staged",
    "precommit-msg": "echo 'Pre-commit checks...' && exit 0",
    "prettier": "prettier -c --write 'src/*'",
    "prod": "vite --mode prod",
    "serve": "vite preview"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{jsx,less,md,json}": [
      "prettier --write"
    ],
    "*.ts?(x)": [
      "prettier --parser=typescript --write"
    ]
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 10"
  ],
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^7.0.0",
    "@fortawesome/free-regular-svg-icons": "^7.0.0",
    "@fortawesome/free-solid-svg-icons": "^7.0.0",
    "@fortawesome/react-fontawesome": "^0.2.3",
    "@nuxt/kit": "latest",
    "@testing-library/jest-dom": "latest",
    "@types/react-responsive": "^9.0.0",
    "@vueuse/core": "latest",
    "antd-mobile": "latest",
    "antd-mobile-icons": "latest",
    "axios": "latest",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "framer-motion": "^12.23.12",
    "fs": "latest",
    "history": "latest",
    "jiti": "latest",
    "lib-flexible": "latest",
    "lightningcss": "latest",
    "lucide-react": "^0.454.0",
    "next": "15.2.4",
    "path": "latest",
    "react": "^18",
    "react-dom": "^18",
    "react-responsive": "^10.0.1",
    "react-router-dom": "latest",
    "react-swipeable": "^7.0.2",
    "react-transition-group": "latest",
    "rmox": "latest",
    "rolldown": "latest",
    "rollup": "latest",
    "sass": "latest",
    "sass-embedded": "latest",
    "stylus": "latest",
    "sugarss": "latest",
    "tailwind-merge": "^2.5.5",
    "tailwindcss-animate": "^1.0.7",
    "tsx": "latest",
    "yaml": "latest",
    "zustand": "^5.0.7"
  },
  "devDependencies": {
    "@types/node": "^22",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "@types/react-transition-group": "^4.4.12",
    "@typescript-eslint/eslint-plugin": "^8.38.0",
    "@typescript-eslint/parser": "^8.38.0",
    "@vitejs/plugin-legacy": "^5.2.8",
    "@vitejs/plugin-react-swc": "^3.6.0",
    "autoprefixer": "^10.4.21",
    "consola": "^3.4.2",
    "eslint": "^9.32.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^5.5.3",
    "eslint-plugin-promise": "^7.2.1",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-standard": "^5.0.0",
    "less": "^4.4.0",
    "less-vars-to-js": "^1.3.0",
    "lint-staged": "^16.1.2",
    "postcss": "^8.5",
    "postcss-pxtorem": "^6.1.0",
    "prettier": "^3.6.2",
    "rollup-plugin-visualizer": "^6.0.3",
    "stylelint": "^16.23.0",
    "stylelint-config-standard": "^39.0.0",
    "tailwindcss": "^3.4.1",
    "terser": "^5.43.1",
    "typescript": "^5",
    "unplugin-auto-import": "^0.17.0",
    "vite": "^5.2.8",
    "vite-plugin-compression": "^0.5.1",
    "vite-plugin-windicss": "^1.9.4"
  }
}
